<?xml version="1.0" encoding="utf-8" ?>
<toolkit:Popup
    x:Class="JuegoMarvel.ModuloLogin.View.PopupErroresCrearCuenta"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
    xmlns:viewModels="clr-namespace:JuegoMarvel.ModuloLogin.ViewModel"
    Color="Transparent">
    <toolkit:Popup.BindingContext>
        <viewModels:PopupErroresCrearCuentaViewModel />
    </toolkit:Popup.BindingContext>

    <VerticalStackLayout>
        <Label Text="{Binding ContenidoTexto}" />
        <Button Clicked="ButtonOk_Clicked" Text="Ok" />
    </VerticalStackLayout>
</toolkit:Popup>